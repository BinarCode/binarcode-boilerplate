includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    paths:
        - app
    # The level 8 is the highest level
    level: 6
    treatPhpDocTypesAsCertain: false
    excludePaths:
        - app/Nova/*/Actions/*
        - app/Models/*
        - app/Console/Commands/OneTime/*
    ignoreErrors:
    checkMissingIterableValueType: false
