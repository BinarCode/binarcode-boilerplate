{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "binaryk/laravel-restify": "^5.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.7",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "myclabs/php-enum": "^1.8",
        "rector/rector": "^0.11.5",
        "rector/rector-laravel": "^0.11.1",
        "spatie/data-transfer-object": "^3.7",
        "spatie/laravel-ray": "^1.26"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^2.19",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.12",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "pestphp/pest-plugin-watch": "1.x-dev",
        "phpunit/phpunit": "^9.3.3",
        "psalm/plugin-laravel": "^1.4",
        "spatie/laravel-tail": "^4.3",
        "spatie/ray": "^1.22",
        "stechstudio/laravel-php-cs-fixer": "^2.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "rector": "./vendor/bin/rector process app",
        "format": "./vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "fresh": "php artisan migrate:fresh --seed",
        "psalm": "./vendor/bin/psalm -c psalm.xml",
        "test": "php artisan test --parallel",
        "pest": "./vendor/bin/pest --watch",
        "larastan": "vendor/bin/phpstan analyse --memory-limit=2G"
    }
}
